<!DOCTYPE html>
<html class="light" lang="fr"><head>
<meta charset="utf-8"/>
<meta content="width=device-width, initial-scale=1.0" name="viewport"/>
<title><%= typeof title !== 'undefined' ? title : 'CarParts France' %></title>
<script>
  (function () {
    document.documentElement.classList.add('js');
  })();
</script>
<style>
  html.js:not(.icons-ready) .material-symbols-outlined,
  html.js:not(.icons-ready) .material-symbols-rounded {
    visibility: hidden;
  }
</style>
<% if (typeof metaRobots !== 'undefined' && metaRobots) { %>
<meta name="robots" content="<%= metaRobots %>"/>
<% } %>
<% if (typeof metaDescription !== 'undefined' && metaDescription) { %>
<meta name="description" content="<%= metaDescription %>"/>
<% } %>
<% if (typeof canonicalUrl !== 'undefined' && canonicalUrl) { %>
<link rel="canonical" href="<%= canonicalUrl %>"/>
<% } %>
<% if (typeof ogTitle !== 'undefined' && ogTitle) { %>
<meta property="og:title" content="<%= ogTitle %>"/>
<% } %>
<% if (typeof ogDescription !== 'undefined' && ogDescription) { %>
<meta property="og:description" content="<%= ogDescription %>"/>
<% } %>
<% if (typeof ogUrl !== 'undefined' && ogUrl) { %>
<meta property="og:url" content="<%= ogUrl %>"/>
<% } %>
<% if (typeof ogSiteName !== 'undefined' && ogSiteName) { %>
<meta property="og:site_name" content="<%= ogSiteName %>"/>
<% } %>
<% if (typeof ogImage !== 'undefined' && ogImage) { %>
<meta property="og:image" content="<%= ogImage %>"/>
<% } %>
<meta property="og:type" content="<%= (typeof ogType !== 'undefined' && ogType) ? ogType : 'website' %>"/>
<% if (typeof ogArticlePublishedTime !== 'undefined' && ogArticlePublishedTime) { %>
<meta property="article:published_time" content="<%= ogArticlePublishedTime %>"/>
<% } %>
<% if (typeof ogArticleModifiedTime !== 'undefined' && ogArticleModifiedTime) { %>
<meta property="article:modified_time" content="<%= ogArticleModifiedTime %>"/>
<% } %>
<meta name="twitter:card" content="summary_large_image"/>
<% if (typeof ogTitle !== 'undefined' && ogTitle) { %>
<meta name="twitter:title" content="<%= ogTitle %>"/>
<% } %>
<% if (typeof ogDescription !== 'undefined' && ogDescription) { %>
<meta name="twitter:description" content="<%= ogDescription %>"/>
<% } %>
<% if (typeof ogImage !== 'undefined' && ogImage) { %>
<meta name="twitter:image" content="<%= ogImage %>"/>
<% } %>
<% if (typeof jsonLd !== 'undefined' && jsonLd) { %>
<script type="application/ld+json"><%- jsonLd %></script>
<% } %>
<link rel="preconnect" href="https://cdn.tailwindcss.com"/>
<script src="https://cdn.tailwindcss.com?plugins=forms,typography,container-queries,line-clamp"></script>
<link rel="preconnect" href="https://fonts.googleapis.com"/>
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin/>
<link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&amp;display=swap" rel="stylesheet"/>
<link href="https://fonts.googleapis.com/css2?family=Outfit:wght@500;600;700&amp;display=swap" rel="stylesheet"/>
<link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:wght,FILL@100..700,0..1&amp;display=swap" rel="stylesheet"/>
<link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200&amp;display=swap" rel="stylesheet"/>
<script>
  (function () {
    var el = document.documentElement;
    if (!el.classList.contains('js')) el.classList.add('js');
    if (!document.fonts || typeof document.fonts.load !== 'function') {
      el.classList.add('icons-ready');
      return;
    }

    var done = false;
    function markReady() {
      if (done) return;
      done = true;
      el.classList.add('icons-ready');
    }

    Promise.all([
      document.fonts.load('1em "Material Symbols Outlined"'),
      document.fonts.load('1em "Material Symbols Rounded"'),
    ])
      .then(markReady)
      .catch(markReady);

    setTimeout(markReady, 2000);
  })();
</script>
<script id="tailwind-config">
        tailwind.config = {
            darkMode: "class",
            theme: {
                extend: {
                    colors: {
                        "primary": "#ec1313",
                        "primary-hover": "#B71C1C",
                        "dark-grey": "#1a1a1a",
                        "footer-red": "#b31d1d",
                        "background-light": "#F9FAFB",
                        "background-dark": "#111827",
                        "surface-light": "#FFFFFF",
                        "surface-dark": "#1F2937",
                        "accent-dark": "#111827",
                        "border-light": "#E5E7EB",
                        "border-dark": "#374151",
                    },
                    fontFamily: {
                        "display": ["Inter", "sans-serif"],
                        "body": ["Inter", "sans-serif"],
                    },
                    boxShadow: {
                        "premium": "0 25px 60px -15px rgba(0, 0, 0, 0.05), 0 15px 30px -10px rgba(0, 0, 0, 0.03)",
                        "premium-hover": "0 35px 70px -15px rgba(0, 0, 0, 0.1), 0 20px 40px -12px rgba(0, 0, 0, 0.06)",
                    },
                    borderRadius: {
                        "DEFAULT": "0.25rem",
                        "lg": "0.5rem",
                        "xl": "0.75rem",
                        "2xl": "1rem",
                        "3xl": "2rem",
                        "full": "9999px"
                    },
                },
            },
        }
    </script>
<style type="text/tailwindcss">
        html.js:not(.icons-ready) .material-symbols-outlined,
        html.js:not(.icons-ready) .material-symbols-rounded {
            visibility: hidden;
        }
        .material-symbols-outlined {
            font-variation-settings: 'FILL' 0, 'wght' 400, 'GRAD' 0, 'opsz' 24;
        }
        .no-scrollbar::-webkit-scrollbar {
            display: none;
        }
        .no-scrollbar {
            -ms-overflow-style: none;
            scrollbar-width: none;
        }
        details[open] {
            @apply border-primary ring-1 ring-primary/20;
        }
        details summary::-webkit-details-marker {
            display: none;
        }
        .hero-workshop {
            background: linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.5)), url('https://images.unsplash.com/photo-1486262715619-67b85e0b08d3?q=80&w=2072&auto=format&fit=crop');
            background-size: cover;
            background-position: center;
        }
        .zoom-area:hover img {
            transform: scale(1.5);
            cursor: zoom-in;
        }
        html {
            text-rendering: optimizeLegibility;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }
    </style>
</head>
<body class="bg-[#f6f7fb] font-display text-slate-900 transition-colors duration-200 antialiased overflow-x-hidden">
<div class="relative flex min-h-screen w-full flex-col">
